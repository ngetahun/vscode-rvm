{
    "name": "ruby-env",
    "displayName": "rvm",
    "description": "Rvm support for vs-code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "rvm.current",
                "title": "Current Ruby",
                "category": "Rvm"
            },
            {
                "command": "",
                "title": "",
                "category": ""
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && jest",
        "jest": "jest"
    },
    "devDependencies": {
        "@types/jest": "^23.3.10",
        "@types/node": "^8.10.25",
        "event-stream": "4.0.1",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.5",
        "tslint": "^6.1.3",
        "typescript": "^2.6.1",
        "vscode": "^1.1.37"
    }
}
